{
  "name": "vsc-dotfiles",
  "publisher": "bree",
  "icon": "images/icon.png",
  "displayName": "vsc-dotfiles",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsc-dotfiles.searchWordUnderCursor",
        "title": "Search Word Under Cursor"
      },

      {
        "command": "vsc-dotfiles.toggleCasing",
        "title": "Toggle Casing"
      },

      {
        "command": "vsc-dotfiles.renameCurrentFile",
        "title": "Rename Current File..."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
